@if (string.IsNullOrWhiteSpace(Url) is false)
{
    <MudAvatar Image="@Url" />
}
else
{
    <MudAvatar Color="Color.Secondary" Variant="Variant.Outlined">
        <MudIcon Icon="@Icons.Material.Filled.Person" />
    </MudAvatar>
}

@code {

    [Parameter]
    public string Url { get; set; } = string.Empty;

}