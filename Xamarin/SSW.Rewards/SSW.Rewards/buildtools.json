{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "projectSecrets": {
    "SSW.Rewards": {
      "delimiter": ";",
      "disable": false,
      "className": "Secrets",
      "namespace": "Helpers",
      "properties": [
        {
          "name": "b2cClientId",
          "type": "String",
          "isArray": false
        },
        {
          "name": "b2cDomain",
          "type": "String",
          "isArray": false
        },
        {
          "name": "b2cSigninPolicy",
          "type": "String",
          "isArray": false
        },
        {
          "name": "b2cResetPolicy",
          "type": "String",
          "isArray": false
        },
        {
          "name": "b2cTenantName",
          "type": "String",
          "isArray": false
        }
      ]
    }
  }
}